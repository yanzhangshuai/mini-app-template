{
  "name": "taro-vue3",
  "version": "1.0.0",
  "repository": "https://github.com/yanzhangshuai/mini-app-template.git",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "description": "taro-vue3",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build": "               taro build --type weapp",
    "build:weapp": "         taro build --type weapp",
    "build:swan": "          taro build --type swan",
    "build:alipay": "        taro build --type alipay",
    "build:tt": "            taro build --type tt",
    "build:h5": "            taro build --type h5",
    "build:rn": "            taro build --type rn",
    "build:qq": "            taro build --type qq",
    "build:jd": "            taro build --type jd",
    "build:quickapp": "      taro build --type quickapp",
    "dev": "                 npm run build:weapp -- --watch",
    "devtool": "             npm run build:weapp -- --watch --devtool",
    "dev:weapp": "           npm run build:weapp -- --watch",
    "dev:swan": "            npm run build:swan -- --watch",
    "dev:alipay": "          npm run build:alipay -- --watch",
    "dev:tt": "              npm run build:tt -- --watch",
    "dev:h5": "              npm run build:h5 -- --watch",
    "dev:rn": "              npm run build:rn -- --watch",
    "dev:qq": "              npm run build:qq -- --watch",
    "dev:jd": "              npm run build:jd -- --watch",
    "dev:quickapp": "        npm run build:quickapp -- --watch",
    "lint:js": "             eslint ./{src,type}/**/*.{ts,tsx,vue} --fix",
    "lint": "                npm-run-all --parallel lint:*"
  },
  "eslintConfig": {
    "extends": "./standard/.eslintrc.js"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@nutui/nutui-taro": "^3.1.18",
    "@pinia/plugin-debounce": "^0.0.2",
    "@tarojs/components": "3.4.4",
    "@tarojs/taro": "3.4.4",
    "dayjs": "^1.11.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.13",
    "typescript": "^4.6.3",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/runtime": "^7.17.9",
    "@tarojs/mini-runner": "3.4.4",
    "@tarojs/plugin-framework-vue3": "^3.4.4",
    "@tarojs/plugin-html": "^3.4.4",
    "@tarojs/plugin-vue-devtools": "^3.4.4",
    "@tarojs/runtime": "^3.4.4",
    "@tarojs/webpack-runner": "3.4.4",
    "@types/lodash-es": "^4.17.6",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.4",
    "eslint": "^8.13.0",
    "eslint-config-taro": "3.4.4",
    "eslint-plugin-vue": "^8.6.0",
    "npm-run-all": "^4.1.5",
    "stylelint": "^14.6.1",
    "taro-plugin-pinia": "^1.0.0",
    "vue-loader": "^17.0.0"
  }
}
